<?php

$output1 = array();
$output2 = array();
$arr1 = array(10, 20, 30);
$arr2 = array("a" => 10, "b" => 20, "c" => 30);
//
for($i=0; $i<count($arr1); $i++) {
  $output1[$i] = $arr1[$i] + 10;
}
//dd($output1);
$map = array_map(fn(int $result): int => $result + 10, $arr2);
foreach($map as $key => $value) {
  $output2[$key] = $value;
}
//dd($output2);
//

